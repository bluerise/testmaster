#!/usr/local/bin/expect -f

set timeout -1

spawn tail -n 0 -f /var/log/daemon
#          "Jul 23 23:23:39 testmaster tftpd[30940]: 10.0.1.1: read request for '/bsd'"
expect -re ".* tftpd.* $env(ipaddr): read request for '/bsd'"
set timeout 60
expect -re ".* tftpd.* $env(ipaddr): read request for '/bsd'"
exit 0
